{
  "global": {
    "version": "v702"
  },
  "rules": {
    "keywords_upper": {
      "enabled": true,
      "ignoreExceptions": true
    },
    "check_variables": {
      "enabled": false,
      "exclude": []
    },
    "line_length": {
      "enabled": true,
      "length": 80
    },
    "start_at_tab": {
      "enabled": true
    }
  }
}
